{
  "name": "xkshop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "npx jetify",
    "release:install": "cd android && ./gradlew installRelease",
    "release": "date \"+%H:%M:%S\" && npm run logger:disable && cd android && ./gradlew assembleRelease && date \"+%H:%M:%S\" && open ./app/build/outputs/apk/release/",
    "release:win": "cd android && gradlew assembleRelease && gradlew installRelease",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output release_ios/main.jsbundle --assets-dest release_ios/ && rm -r release_ios/assets/node_modules/react-native",
    "build:release": "npm run logger:disable && npm run bundle:ios && cd ios && bash ./build.sh Release",
    "build:debug": "npm run logger:disable && npm run bundle:ios && cd ios && bash ./build.sh Debug",
    "envir:beta": "node scripts/index.js -r envir=3 netHeader=https",
    "envir:test": "node scripts/index.js -r envir=2 netHeader=https",
    "envir:dev": "node scripts/index.js -r envir=1 netHeader=https",
    "envir:release": "node scripts/index.js -r envir=0 netHeader=https",
    "logger:enable": "node scripts/index.js -r enableLogger=1",
    "logger:disable": "node scripts/index.js -r enableLogger=0",
    "pod:install": "cd ios/ && pod install --verbose --no-repo-update",
    "pod:update": "cd ios/ && pod update --verbose --no-repo-update",
    "detox:ios:build": "detox build --configuration ios.sim.debug",
    "detox:ios:test": "detox test  --configuration ios.sim.debug",
    "detox:android:build": "detox build --configuration android.emu.debug",
    "detox:android:test": "detox test  --configuration android.emu.debug"
  },
  "dependencies": {
    "@react-native-community/cameraroll": "^1.1.1",
    "@scxkkj/react-native-audio-player": "^0.2.8",
    "@scxkkj/react-native-wechat": "^1.9.14",
    "axios": "^0.18.0",
    "buffer": "^5.2.1",
    "crypto-js": "^3.1.9-1",
    "deprecated-react-native-listview": "^0.0.5",
    "dva-loading": "^3.0.2",
    "jcore-react-native": "1.3.2",
    "jpush-react-native": "2.5.3",
    "mathjs": "^5.4.2",
    "moment": "^2.22.2",
    "querystring": "^0.2.0",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-cli": "^2.0.1",
    "react-native-device-info": "^2.3.2",
    "react-native-exception-handler": "^2.10.6",
    "react-native-fs": "^2.14.1",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-photo-view-ex": "^1.1.0",
    "react-native-picker-xk": "^4.3.9",
    "react-native-qrcode-svg": "^5.1.1",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-splash-screen": "^3.1.1",
    "react-native-storage": "^0.2.2",
    "react-native-svg": "^9.5.3",
    "react-native-swiper": "^1.5.13",
    "react-native-switch-pro": "^1.0.2-beta",
    "react-native-video": "^5.0.0",
    "react-native-view-shot": "^3.0.2",
    "react-native-webview": "^6.6.0",
    "react-navigation": "^2.17.0",
    "react-navigation-redux-helpers": "^2.0.9",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "rn-dva": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
